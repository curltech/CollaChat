  height: 40px;
  width: 40px;
  min-width: 40px;
}
.list-message{
  position: relative;
}
.list-message .q-message-name{
  position: absolute;
  right: 15px;
  top: 30px;
}
.q-message-sent .q-message-container {
  padding-left: 40px;
}
.q-message-received .q-message-container {
  padding-right: 40px;
}
.q-message-text {
  font-size: 15px;
  min-height: 40px !important;
  display: flex;
  align-items: center;
  justify-content: space-around;
  flex-direction: column;
  letter-spacing: 0.5px;
}
.q-message-label {
  font-size: 12px;
  color: #9E9E9E !important; // grey-6
  margin: 20px 0px;
}
.q-message-text--sent {
  background-color: #51DBE9;
