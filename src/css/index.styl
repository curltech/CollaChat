    padding-top: var(--navigatorTabPaddingTop);
  }
  .q-message-text {
    font-size: 17px;
  }
  .q-message-label {
    font-size: 14px;
  }
  .message-editor {
    font-size: 17px;
  }
}
.platform-android {
  font-size: 15px;
  .q-toolbar__title {
    font-size: 15px;
    font-weight: bold;
  }
  .q-table tbody td {
    font-size: 12px;
  }
  .q-field {
    font-size: 15px;
  }
  .q-btn__content {
    font-size: 15px;
  }
  .note-title {
    font-size: 15px;
  }
  .note-caption {
    font-size: 13px;
  }
  .note-caption-small {
    font-size: 12px;
  }
  .navigatorTabMobileStyle .q-tab__content {
    padding-top: 4px;
  }
  .q-message-label {
    font-size: 12px;
  }
}

.c-field{
  width: 100% !important;
  border: none;
}
.c-field .q-field__control:before {
  border-bottom: none !important;
  transition: none;
  background: none !important;
}
.c-field .q-field__control:after {
  background: none;
}
