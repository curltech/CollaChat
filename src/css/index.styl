  }
  .text-caption {
    font-size: 14px;
  }
  .q-table th {
    font-size: 14px;
  }
  .q-table tbody td {
    font-size: 14px;
  }
  .q-field {
    font-size: 17px;
  }
  .q-btn__content {
    font-size: 17px;
  }
  .q-tab__label {
    font-size: 16px;
  }
  .note-title {
    font-size: 17px;
  }
  .note-caption {
    font-size: 15px;
  }
  .note-caption-small {
    font-size: 14px;
  }
